public class reversearray {
	public static void main(String[] args) {
		int [] arr = new int [] {25, 11, 7, 75, 56};
		int i=0;
		int j = arr.length-1;
		for(;i<j;) {
			int temp;
			temp = arr[i];
			arr[i]=arr[j];
			arr[j]=temp;
			i++;
			j--;
			
		}
		for(i=0; i <arr.length;i++) {
			System.out.println(arr[i]);
		}
	}
}
