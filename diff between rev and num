import java.util.*;
public class Main
{
    public static void main (String[] args) {
       Scanner sc = new Scanner(System.in);
       int n = sc.nextInt();
       int rem,sum=0;
       int num = n;
        while(num>0)
        {
            rem = num%10;
            sum =(sum*10)+rem;
            num = num/10;
        }
       System.out.println((n-sum));
    }
}
