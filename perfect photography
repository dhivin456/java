import java.util.*;
import java.lang.Math;
class Main
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int c=sc.nextInt();
        int d=sc.nextInt();
        double side=Math.sqrt(d);
        double r=Math.sqrt(2);
        double x=((a*r)-(side*r))/Math.abs(b-c);
        System.out.printf("%.2f",(x*100)/100);
    }
}
