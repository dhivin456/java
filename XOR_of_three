import java.util.*;
public class Main
{
    public static void main (String[] args) {
        /* code */
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int a[] = new int[n];
        for(int i = 0; i<n; i++)
        {
            a[i] = sc.nextInt();
        }
        int temp = a[0];
        for(int j = 1;j<n;j++)
        {
            temp ^= a[j];
        }
        System.out.println(temp);
    }
}
