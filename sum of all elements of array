import java.util.*;
public class Main
{
    public static void main(String args[])
    {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int my_nw = 0;
        for(int i = 0; i<n; i++)
        {
            my_nw += sc.nextInt();
        }
        System.out.println(my_nw);
    }
}
